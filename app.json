{
  "expo": {
    "name": "Ervie",
    "slug": "ervieexpoapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.jpg",
    "scheme": "ervieapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.ervie.portfolio",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "com.ervie.portfolio",
      "versionCode": 1,
      "adaptiveIcon": {
        "backgroundColor": "#f6f2ff",
        "foregroundImage": "./assets/images/adaptive-icon.jpg"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#f6f2ff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "d20a0c69-e222-4acb-a66f-5a0e9e73ed78" 
      }
    }
  }
}